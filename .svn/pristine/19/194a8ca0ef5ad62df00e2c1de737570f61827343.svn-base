import wx

app = wx.App()

frame = wx.Frame(None, -1, 'simple.py')
frame.Show()
frame.SetToolTip(wx.ToolTip('This is a frame'))
frame.SetCursor(wx.StockCursor(wx.CURSOR_MAGNIFIER))
